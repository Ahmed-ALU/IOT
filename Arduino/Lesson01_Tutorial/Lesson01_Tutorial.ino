void setup() {
  // put your setup code here, to run once:
pinMode(8, OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  delay(800);
  digitalWrite(8, HIGH);
  delay(700);
  digitalWrite(8, LOW);
  delay(600);
  digitalWrite(8, HIGH);
  delay(500);
  digitalWrite(8, LOW);
  delay(400);
  digitalWrite(8, HIGH);
  delay(300);
  digitalWrite(8, LOW);
  delay(200);
  digitalWrite(8, HIGH);
  delay(100);
  digitalWrite(8, LOW);
  delay(50);
  digitalWrite(8, HIGH);
  delay(25);
  digitalWrite(8, LOW);
}
